#include <iostream>
#include <string>
#include <vector>
using namespace std;
class student {
    int group_number;
    string surname, name, lastname;
    int wasborn, o1, o2, o3, o4, o5;
public:
    string getSurname() {
        return surname;
    }
    string getName() {
        return name;
    }
    string getLastname() {
        return lastname;
    }
    int getYear() {
        return wasborn;
    }
    void sort(student* a, int n) {
        student temp;
        int i, j;
        for (int i = 1; i <= n; i++) {
            j = i;
            while (a[j] < a[j - 1]) {
                temp = a[j];
                a[j] = a[j - 1];
                a[j - 1] = temp;
                j--;
            }
        }
    }
};
bool sravname(student first, student second, int k = 0) {
    int size;
    string s1, s2;
    if (k == 0) {
        s1 = first.getSurname();
        s2 = second.getSurname();
    }
    else if (k == 1) {
        s1 = first.getName();
        s2 = second.getName();
    }
    else if (k == 2) {
        s1 = first.getLastname();
        s2 = second.getLastname();
    }
    if (s1 < s2) size = s1.size();
    else size = s2.size();
    for (int i = 0; i < size; i++) {
        if (s1[i] < s2[i])
            return true;
        else if (s1[i] > s2[i])
            return false;
    }
    if (size < s2.size())
        return true;
    else if (size > s2.size())
        return false;
    else {
        if (k < 2)
            sravname(first, second, ++k);
        else return false;
    }
}
bool operator<(student first, student second) {
    if (sravname(first, second))
        if (first.getYear() < second.getYear())
            return true;
}
int main() {
    student A[10];
}
